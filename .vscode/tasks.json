{
    "tasks": [
        {
            "type": "cppbuild",
            "label": "C/C++: gcc",
            "command": "/usr/bin/gcc",
            "args": [
                "-fdiagnostics-color=always",
                "-g",
                "-I${fileDirname}/../../src",
                "-o",
                "${fileDirname}/../../build/test/util/${fileBasenameNoExtension}",
                "${file}",
                "-L${fileDirname}/../../build",
                "-lyuyu"
            ],
            "options": {
                "cwd": "${fileDirname}"
            },
            "problemMatcher": [
                "$gcc"
            ],
            "group": {
                "kind": "build",
                "isDefault": true
            },
            "detail": "调试器生成的任务。"
        }
    ],
    "version": "2.0.0"
}